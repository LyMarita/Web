
	<table border="1">
		<tr>
			
			<td><%= @post.id %></td>
			<td><%= @post.title %></td>
			<td><%= @post.description %></td>
			<td><%= @post.date %></td>
			<td><%= @post.publish %></td>
			<td><img src="../../assets<%=@post.image_file_name%>" width="100px"/></td>
			<td><%= link_to "Edit", edit_post_path(@post.id) %></td>
			<td><%= link_to "Delete",post_path(@post.id),method: :delete, data: {confirm: "Do you want to delete?"} %> </td>
		</tr>
	</table>			
</div>